# .env.pp-production - Build React pour PRÉ-PRODUCTION
# Utilisation : npm run build:pp

# API sur le même domaine
REACT_APP_API_BASE_URL=/api
REACT_APP_BACKEND_URL=

# Logs activés en pré-prod
REACT_APP_DEBUG_URLS=true

# Indicateur d'environnement
REACT_APP_ENVIRONMENT=pre-production
REACT_APP_ENVIRONMENT_LABEL=PRÉ-PROD
REACT_APP_ENVIRONMENT_COLOR=#ff9800

# Pas de base path
REACT_APP_BASE_PATH=