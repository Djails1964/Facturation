const NotificationProvider = ({
  children,
  initialConfig = {}
}) => {
  const [notifications, setNotifications] = (0, _react.useState)([]);
  const [config, setConfig] = (0, _react.useState)(() => {
    const normalizedConfig = (0, _booleanHelper.normalizeBooleanFields)({
      ...DEFAULT_CONFIG,
      ...initialConfig
    }, Object.keys(DEFAULT_CONFIG));
    log.debug('✅ Configuration notifications normalisée:', normalizedConfig);
    return normalizedConfig;
  });

  // Mise à jour de la configuration
  const updateConfig = (0, _react.useCallback)(newConfig => {
    setConfig(prevConfig => {
      const updatedConfig = (0, _booleanHelper.normalizeBooleanFields)({
        ...prevConfig,
        ...newConfig
      }, Object.keys(DEFAULT_CONFIG));
      log.debug('✅ Configuration notifications mise à jour:', updatedConfig);
      return updatedConfig;
    });
  }, []);

  // Normalisation d'une notification
  const normalizeNotification = (0, _react.useCallback)(notification => {
    const booleanFields = ['isPersistent', 'isDismissible', 'showIcon', 'showCloseButton', 'showProgressBar', 'autoClose', 'isRead', 'isImportant'];
    const normalized = (0, _booleanHelper.normalizeBooleanFields)(notification, booleanFields);

    // Appliquer les valeurs par défaut de la configuration
    if (normalized.showIcon === undefined) normalized.showIcon = config.showIcons;
    if (normalized.showCloseButton === undefined) normalized.showCloseButton = config.showCloseButton;
    if (normalized.showProgressBar === undefined) normalized.showProgressBar = config.showProgressBar;
    if (normalized.isDismissible === undefined) normalized.isDismissible = true;
    return normalized;
  }, [config]);

  // Vérification des doublons
  const isDuplicateNotification = (0, _react.useCallback)((message, type) => {
    if ((0, _booleanHelper.toBoolean)(config.allowDuplicates)) return false;
    return notifications.some(notification => notification.message === message && notification.type === type);
  }, [notifications, config.allowDuplicates]);

  // Ajouter une notification
  const addNotification = (0, _react.useCallback)((type, message, options = {}) => {
    // Vérifier les doublons
    if (isDuplicateNotification(message, type)) {
      log.debug('ℹ️ Notification dupliquée ignorée:', {
        type,
        message
      });
      return null;
    }
    const id = Date.now().toString() + Math.random().toString(36).substr(2, 9);

    // Normalisation des options
    const normalizedOptions = normalizeNotification({
      duration: 5000,
      action: null,
      isPersistent: false,
      isDismissible: true,
      isImportant: type === NOTIFICATION_TYPES.ERROR,
      ...options
    });

    // Gestion des notifications critiques
    if ((0, _booleanHelper.toBoolean)(config.persistCritical) && type === NOTIFICATION_TYPES.ERROR) {
      normalizedOptions.isPersistent = true;
      normalizedOptions.duration = 0;
    }
    const notification = {
      id,
      type,
      message,
      timestamp: new Date().toISOString(),
      ...normalizedOptions,
      config
    };
    setNotifications(prevNotifications => {
      const newNotifications = (0, _booleanHelper.toBoolean)(config.stackFromTop) ? [notification, ...prevNotifications] : [...prevNotifications, notification];
      return newNotifications;
    });
    return id;
  }, [config, isDuplicateNotification, normalizeNotification]);

  // Méthodes d'ajout par type
  const showSuccess = (0, _react.useCallback)((message, options = {}) => addNotification(NOTIFICATION_TYPES.SUCCESS, message, {
    duration: 3000,
    ...options
  }), [addNotification]);
  const showError = (0, _react.useCallback)((message, options = {}) => addNotification(NOTIFICATION_TYPES.ERROR, message, {
    duration: 0,
    isPersistent: true,
    isImportant: true,
    ...options
  }), [addNotification]);
  const showWarning = (0, _react.useCallback)((message, options = {}) => addNotification(NOTIFICATION_TYPES.WARNING, message, {
    duration: 7000,
    isImportant: true,
    ...options
  }), [addNotification]);
  const showInfo = (0, _react.useCallback)((message, options = {}) => addNotification(NOTIFICATION_TYPES.INFO, message, {
    duration: 5000,
    ...options
  }), [addNotification]);

  // Méthodes utilitaires avancées
  const showPersistent = (0, _react.useCallback)((type, message, options = {}) => addNotification(type, message, {
    ...options,
    isPersistent: true,
    duration: 0
  }), [addNotification]);
  const showTemporary = (0, _react.useCallback)((type, message, duration = 3000, options = {}) => addNotification(type, message, {
    ...options,
    isPersistent: false,
    duration
  }), [addNotification]);

  // Fermeture des notifications
  const closeNotification = (0, _react.useCallback)(id => {
    setNotifications(prevNotifications => prevNotifications.filter(notification => notification.id !== id));
  }, []);
  const closeByType = (0, _react.useCallback)(type => {
    setNotifications(prevNotifications => prevNotifications.filter(notification => notification.type !== type));
  }, []);
  const closeNonPersistent = (0, _react.useCallback)(() => {
    setNotifications(prevNotifications => prevNotifications.filter(notification => (0, _booleanHelper.toBoolean)(notification.isPersistent)));
  }, []);
  const clearAll = (0, _react.useCallback)(() => {
    setNotifications([]);
  }, []);

  // Statistiques
  const getStats = (0, _react.useCallback)(() => {
    const stats = {
      total: notifications.length,
      byType: {},
      persistent: 0,
      dismissible: 0,
      important: 0
    };
    notifications.forEach(notification => {
      stats.byType[notification.type] = (stats.byType[notification.type] || 0) + 1;
      if ((0, _booleanHelper.toBoolean)(notification.isPersistent)) stats.persistent++;
      if ((0, _booleanHelper.toBoolean)(notification.isDismissible)) stats.dismissible++;
      if ((0, _booleanHelper.toBoolean)(notification.isImportant)) stats.important++;
    });
    return stats;
  }, [notifications]);

  // Valeur du contexte
  const contextValue = {
    notifications,
    config,
    showSuccess,
    showError,
    showWarning,
    showInfo,
    showPersistent,
    showTemporary,
    closeNotification,
    closeByType,
    closeNonPersistent,
    clearAll,
    updateConfig,
    getStats,
    isConfigEnabled: key => (0, _booleanHelper.toBoolean)(config[key]),
    hasNotifications: () => notifications.length > 0,
    hasImportantNotifications: () => notifications.some(n => (0, _booleanHelper.toBoolean)(n.isImportant)),
    hasPersistentNotifications: () => notifications.some(n => (0, _booleanHelper.toBoolean)(n.isPersistent))
  };
  return /*#__PURE__*/_react.default.createElement(NotificationContext.Provider, {
    value: contextValue
  }, children, /*#__PURE__*/_react.default.createElement("div", {
    className: `notifications-container ${(0, _booleanHelper.toBoolean)(config.stackFromTop) ? 'stack-top' : 'stack-bottom'}`
  }, notifications.map(notification => /*#__PURE__*/_react.default.createElement(Notification, _extends({
    key: notification.id
  }, notification, {
    onClose: closeNotification
  })))));
