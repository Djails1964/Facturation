const Notification = ({
  id,
  type,
  message,
  duration = 5000,
  action,
  onClose,
  config = {},
  ...props
}) => {
  const [isPaused, setIsPaused] = (0, _react.useState)(false);
  const timerRef = (0, _react.useRef)(null);

  // Normalisation des propriétés booléennes
  const normalizedProps = (0, _booleanHelper.normalizeBooleanFields)(props, ['isPersistent', 'isDismissible', 'showIcon', 'showCloseButton', 'showProgressBar', 'isImportant']);

  // Icônes par type
  const icons = {
    [NOTIFICATION_TYPES.SUCCESS]: /*#__PURE__*/_react.default.createElement(_fi.FiCheckCircle, {
      size: 20
    }),
    [NOTIFICATION_TYPES.ERROR]: /*#__PURE__*/_react.default.createElement(_fi.FiXCircle, {
      size: 20
    }),
    [NOTIFICATION_TYPES.WARNING]: /*#__PURE__*/_react.default.createElement(_fi.FiAlertTriangle, {
      size: 20
    }),
    [NOTIFICATION_TYPES.INFO]: /*#__PURE__*/_react.default.createElement(_fi.FiInfo, {
      size: 20
    })
  };
  const renderIcon = () => {
    if (!(0, _booleanHelper.toBoolean)(normalizedProps.showIcon)) return null;
    return /*#__PURE__*/_react.default.createElement("div", {
      className: "notification-icon"
    }, icons[type]);
  };

  // Gestion de la fermeture automatique
  (0, _react.useEffect)(() => {
    if (duration > 0 && !normalizedProps.isPersistent && !isPaused) {
      timerRef.current = setTimeout(() => {
        onClose(id);
      }, duration);
      return () => {
        if (timerRef.current) clearTimeout(timerRef.current);
      };
    }
  }, [duration, id, isPaused, normalizedProps.isPersistent, onClose]);

  // Gestion du hover
  const handleMouseEnter = () => setIsPaused(true);
  const handleMouseLeave = () => setIsPaused(false);

  // Gestion du clic sur la notification
  const handleNotificationClick = () => {
    if (normalizedProps.isDismissible && !action) {
      onClose(id);
    }
  };
  return /*#__PURE__*/_react.default.createElement("div", {
    className: `notification notification-${type} ${isPaused ? 'paused' : ''} ${normalizedProps.isPersistent ? 'persistent' : ''}`,
    onClick: handleNotificationClick,
    onMouseEnter: handleMouseEnter,
    onMouseLeave: handleMouseLeave,
    role: "alert",
    "aria-live": type === NOTIFICATION_TYPES.ERROR ? 'assertive' : 'polite',
    "aria-atomic": "true"
  }, renderIcon(), /*#__PURE__*/_react.default.createElement("div", {
    className: "notification-content"
  }, message), action && /*#__PURE__*/_react.default.createElement("button", {
    onClick: e => {
      e.stopPropagation();
      if (action.onAction) action.onAction();
      if (!(0, _booleanHelper.toBoolean)(action.keepOpen)) onClose(id);
    },
    className: "notification-action",
    "aria-label": action.actionLabel || 'Action'
  }, action.actionLabel || 'Action'), normalizedProps.showCloseButton && normalizedProps.isDismissible && /*#__PURE__*/_react.default.createElement("button", {
    onClick: e => {
      e.stopPropagation();
      onClose(id);
    },
    className: "notification-close",
    "aria-label": "Fermer la notification"
  }, "\xD7"), normalizedProps.showProgressBar && duration > 0 && !normalizedProps.isPersistent && /*#__PURE__*/_react.default.createElement("div", {
    className: `notification-progress ${isPaused ? 'paused' : ''}`,
    style: {
      animationDuration: `${duration}ms`,
      animationPlayState: isPaused ? 'paused' : 'running'
    }
  }));
};

