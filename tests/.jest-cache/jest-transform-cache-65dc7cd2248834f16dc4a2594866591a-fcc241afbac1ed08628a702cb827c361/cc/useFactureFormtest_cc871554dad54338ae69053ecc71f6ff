92661c6c0e0a368c0440fd6e3f4d39f5
"use strict";

var _react = require("@testing-library/react");
var _useFactureForm = require("../../../src/components/factures/hooks/useFactureForm");
// tests/unit/hooks/useFactureForm.test.js
// Tests unitaires pour le hook useFactureForm

// Import après les mocks

describe('useFactureForm', () => {
  describe('État initial', () => {
    it('devrait initialiser avec les valeurs par défaut', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      const {
        factureFormState
      } = result.current;
      expect(factureFormState.facture).toEqual({
        idFacture: null,
        numeroFacture: '',
        dateFacture: expect.any(String),
        // Date du jour
        dateEcheance: '',
        idClient: null,
        etat: 'En attente',
        lignes: [],
        ristourne: 0,
        commentaire: ''
      });
      expect(factureFormState.isLoading).toBe(false);
      expect(factureFormState.error).toBeNull();
      expect(factureFormState.isModified).toBe(false);
    });
  });
  describe('setFacture', () => {
    it('devrait mettre à jour la facture complète', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      const nouvelleFacture = createTestFacture();
      (0, _react.act)(() => {
        result.current.setFacture(nouvelleFacture);
      });
      expect(result.current.factureFormState.facture).toEqual(nouvelleFacture);
    });
  });
  describe('updateFactureField', () => {
    it('devrait mettre à jour un champ spécifique', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      (0, _react.act)(() => {
        result.current.updateFactureField('idClient', 5);
      });
      expect(result.current.factureFormState.facture.idClient).toBe(5);
    });
    it('devrait marquer le formulaire comme modifié', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      (0, _react.act)(() => {
        result.current.updateFactureField('ristourne', 10);
      });
      expect(result.current.factureFormState.isModified).toBe(true);
    });
  });
  describe('setLignes', () => {
    it('devrait mettre à jour les lignes de la facture', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      const lignes = [{
        idLigne: 1,
        idService: 1,
        quantite: 2,
        prixUnitaire: 100
      }, {
        idLigne: 2,
        idService: 2,
        quantite: 1,
        prixUnitaire: 200
      }];
      (0, _react.act)(() => {
        result.current.setLignes(lignes);
      });
      expect(result.current.factureFormState.facture.lignes).toEqual(lignes);
    });
  });
  describe('setIsLoading', () => {
    it('devrait mettre à jour l\'état de chargement', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      (0, _react.act)(() => {
        result.current.setIsLoading(true);
      });
      expect(result.current.factureFormState.isLoading).toBe(true);
      (0, _react.act)(() => {
        result.current.setIsLoading(false);
      });
      expect(result.current.factureFormState.isLoading).toBe(false);
    });
  });
  describe('setError', () => {
    it('devrait définir une erreur', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      (0, _react.act)(() => {
        result.current.setError('Une erreur est survenue');
      });
      expect(result.current.factureFormState.error).toBe('Une erreur est survenue');
    });
    it('devrait effacer une erreur', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      (0, _react.act)(() => {
        result.current.setError('Une erreur');
        result.current.setError(null);
      });
      expect(result.current.factureFormState.error).toBeNull();
    });
  });
  describe('resetForm', () => {
    it('devrait réinitialiser le formulaire', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());

      // Modifier le formulaire
      (0, _react.act)(() => {
        result.current.updateFactureField('idClient', 5);
        result.current.updateFactureField('ristourne', 100);
        result.current.setError('Une erreur');
      });

      // Réinitialiser
      (0, _react.act)(() => {
        result.current.resetForm();
      });
      expect(result.current.factureFormState.facture.idClient).toBeNull();
      expect(result.current.factureFormState.facture.ristourne).toBe(0);
      expect(result.current.factureFormState.error).toBeNull();
      expect(result.current.factureFormState.isModified).toBe(false);
    });
  });
  describe('setIsModified', () => {
    it('devrait permettre de définir manuellement isModified', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      (0, _react.act)(() => {
        result.current.setIsModified(true);
      });
      expect(result.current.factureFormState.isModified).toBe(true);
      (0, _react.act)(() => {
        result.current.setIsModified(false);
      });
      expect(result.current.factureFormState.isModified).toBe(false);
    });
  });
  describe('Validation des données', () => {
    it('devrait valider une facture complète', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      const factureValide = createTestFacture();
      (0, _react.act)(() => {
        result.current.setFacture(factureValide);
      });

      // La facture devrait être considérée comme valide
      const {
        facture
      } = result.current.factureFormState;
      expect(facture.idClient).toBeTruthy();
      expect(facture.dateFacture).toBeTruthy();
      expect(facture.lignes.length).toBeGreaterThan(0);
    });
    it('devrait détecter une facture incomplète', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());

      // Facture sans client
      const factureIncomplete = {
        ...createTestFacture(),
        idClient: null,
        lignes: []
      };
      (0, _react.act)(() => {
        result.current.setFacture(factureIncomplete);
      });
      const {
        facture
      } = result.current.factureFormState;
      expect(facture.idClient).toBeNull();
      expect(facture.lignes.length).toBe(0);
    });
  });
  describe('Calculs automatiques', () => {
    it('devrait calculer le total avec ristourne', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureForm.useFactureForm)());
      const facture = createTestFacture({
        montantTotal: 1000,
        ristourne: 100
      });
      (0, _react.act)(() => {
        result.current.setFacture(facture);
      });
      const {
        facture: f
      } = result.current.factureFormState;
      const totalNet = f.montantTotal - f.ristourne;
      expect(totalNet).toBe(900);
    });
  });
});
