"use strict";

// Mock du service FactureService
_getJestObj().mock("../../../src/services/FactureService", () => {
  return jest.fn().mockImplementation(() => _services.mockFactureService);
});

// Mock de useApiCall
_getJestObj().mock("../../../src/hooks/useApiCall", () => ({
  useApiCall: () => ({
    execute: jest.fn(fn => fn()),
    isLoading: false,
    error: null
  })
}));

// Import après les mocks
var _react = require("@testing-library/react");
var _services = require("../../mocks/services");
var _useFactureActions = require("../../../src/components/factures/hooks/useFactureActions");
function _getJestObj() {
  const {
    jest
  } = require("@jest/globals");
  _getJestObj = () => jest;
  return jest;
} // tests/unit/hooks/useFactureActions.test.js
// Tests unitaires pour le hook useFactureActions
describe('useFactureActions', () => {
  beforeEach(() => {
    (0, _services.resetAllMocks)();
  });
  describe('chargerFactures', () => {
    it('devrait charger les factures avec succès', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      let factures;
      await (0, _react.act)(async () => {
        factures = await result.current.chargerFactures(2025);
      });
      expect(_services.mockFactureService.chargerFactures).toHaveBeenCalledWith(2025);
      expect(factures).toHaveLength(2);
    });
    it('devrait enrichir les factures avec les états d\'affichage', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      let factures;
      await (0, _react.act)(async () => {
        factures = await result.current.chargerFactures(2025);
      });

      // Vérifier que les factures ont été enrichies
      expect(factures[0]).toHaveProperty('etatAffichage');
    });
    it('devrait gérer les erreurs de chargement', async () => {
      (0, _services.mockServiceError)(_services.mockFactureService, 'chargerFactures', 'Erreur réseau');
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      await expect(async () => {
        await (0, _react.act)(async () => {
          await result.current.chargerFactures(2025);
        });
      }).rejects.toThrow('Erreur réseau');
    });
  });
  describe('chargerFacture', () => {
    it('devrait charger une facture spécifique', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      let facture;
      await (0, _react.act)(async () => {
        facture = await result.current.chargerFacture(1);
      });
      expect(_services.mockFactureService.getFacture).toHaveBeenCalledWith(1);
      expect(facture).toBeDefined();
      expect(facture.idFacture).toBe(1);
    });
    it('devrait gérer une facture non trouvée', async () => {
      _services.mockFactureService.getFacture.mockResolvedValueOnce({
        success: false,
        message: 'Facture non trouvée'
      });
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      let facture;
      await (0, _react.act)(async () => {
        facture = await result.current.chargerFacture(999);
      });
      expect(facture).toBeNull();
    });
  });
  describe('creerFacture', () => {
    it('devrait créer une facture avec succès', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      const factureData = {
        idClient: 1,
        dateFacture: '2025-01-15',
        lignes: [{
          idService: 1,
          quantite: 2,
          prixUnitaire: 100
        }]
      };
      let response;
      await (0, _react.act)(async () => {
        response = await result.current.creerFacture(factureData);
      });
      expect(_services.mockFactureService.creerFacture).toHaveBeenCalledWith(factureData);
      expect(response.success).toBe(true);
      expect(response.idFacture).toBeDefined();
    });
    it('devrait valider les données obligatoires', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      const factureIncomplete = {
        // Manque idClient
        dateFacture: '2025-01-15'
      };
      _services.mockFactureService.creerFacture.mockRejectedValueOnce(new Error('Client obligatoire'));
      await expect(async () => {
        await (0, _react.act)(async () => {
          await result.current.creerFacture(factureIncomplete);
        });
      }).rejects.toThrow('Client obligatoire');
    });
  });
  describe('modifierFacture', () => {
    it('devrait modifier une facture avec succès', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      const modifications = {
        ristourne: 50
      };
      let response;
      await (0, _react.act)(async () => {
        response = await result.current.modifierFacture(1, modifications);
      });
      expect(_services.mockFactureService.modifierFacture).toHaveBeenCalledWith(1, modifications);
      expect(response.success).toBe(true);
    });
  });
  describe('supprimerFacture', () => {
    it('devrait supprimer une facture avec succès', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      let response;
      await (0, _react.act)(async () => {
        response = await result.current.supprimerFacture(1);
      });
      expect(_services.mockFactureService.supprimerFacture).toHaveBeenCalledWith(1);
      expect(response.success).toBe(true);
    });
  });
  describe('annulerFacture', () => {
    it('devrait annuler une facture avec succès', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      let response;
      await (0, _react.act)(async () => {
        response = await result.current.annulerFacture(1);
      });
      expect(_services.mockFactureService.annulerFacture).toHaveBeenCalledWith(1);
      expect(response.success).toBe(true);
    });
  });
  describe('getProchainNumeroFacture', () => {
    it('devrait récupérer le prochain numéro de facture', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      let numero;
      await (0, _react.act)(async () => {
        numero = await result.current.getProchainNumeroFacture(2025);
      });
      expect(_services.mockFactureService.getProchainNumeroFacture).toHaveBeenCalledWith(2025);
      expect(numero).toBe('004.2025');
    });
  });
  describe('enrichirFacturesAvecEtat', () => {
    it('devrait marquer les factures en retard', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      const factures = [createTestFacture({
        idFacture: 1,
        etat: 'En attente',
        dateEcheance: '2024-01-01' // Date passée
      })];
      const enrichies = result.current.enrichirFacturesAvecEtat(factures);
      expect(enrichies[0].etatAffichage).toBe('Retard');
    });
    it('devrait conserver l\'état pour les factures non en retard', () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      const factures = [createTestFacture({
        idFacture: 1,
        etat: 'Payée',
        dateEcheance: '2030-01-01'
      })];
      const enrichies = result.current.enrichirFacturesAvecEtat(factures);
      expect(enrichies[0].etatAffichage).toBe('Payée');
    });
  });
  describe('actions documents', () => {
    it('devrait imprimer une facture', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      let response;
      await (0, _react.act)(async () => {
        response = await result.current.imprimerFacture(1);
      });
      expect(_services.mockFactureService.imprimerFacture).toHaveBeenCalledWith(1);
      expect(response.success).toBe(true);
    });
    it('devrait envoyer une facture par email', async () => {
      const {
        result
      } = (0, _react.renderHook)(() => (0, _useFactureActions.useFactureActions)());
      const emailData = {
        destinataire: 'test@example.com',
        sujet: 'Votre facture',
        message: 'Veuillez trouver ci-joint...'
      };
      let response;
      await (0, _react.act)(async () => {
        response = await result.current.envoyerFactureParEmail(1, emailData);
      });
      expect(_services.mockFactureService.envoyerFactureParEmail).toHaveBeenCalledWith(1, emailData);
      expect(response.success).toBe(true);
    });
  });
});
