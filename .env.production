# .env.production - Build React pour PRODUCTION
# Utilisation : npm run build (ou npm run build:prod)

# API sur le même domaine
REACT_APP_API_BASE_URL=/api
REACT_APP_BACKEND_URL=

# Logs désactivés en production
REACT_APP_DEBUG_URLS=false

# Indicateur d'environnement
REACT_APP_ENVIRONMENT=production
REACT_APP_ENVIRONMENT_LABEL=
REACT_APP_ENVIRONMENT_COLOR=

# Pas de base path
REACT_APP_BASE_PATH=